{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "flightphp/runway": "^1.1",
    "overclokk/cookie": "^1.0",
    "tracy/tracy": "^2.10",
    "latte/latte": "^3.0",
    "flightphp/active-record": "^0.6.2",
    "byjg/migration": "6.0.x-dev",
    "vlucas/phpdotenv": "^5.6",
    "rollbar/rollbar": "^4.1",
    "rakit/validation": "^1.4",
    "mevdschee/php-crud-api": "^2.14",
    "casbin/casbin": "^v4.0",
    "casbin/database-adapter": "^v2.1",
    "ucscode/table-generator": "^3.2",
    "ext-dom": "*",
    "ext-libxml": "*",
    "cocur/slugify": "^4.6",
    "flightphp/core": "^3.17"
  },
  "require-dev": {
    "flightphp/tracy-extensions": "^0.2.7"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "App\\Commands\\": "app/commands/",
      "App\\Configs\\": "app/configs/",
      "App\\Controllers\\": "app/controllers/",
      "App\\Databases\\": "app/databases/",
      "App\\Helpers\\": "app/helpers/",
      "App\\Records\\": "app/records/",
      "App\\Services\\": "app/services/",
      "App\\Traits\\": "app/traits/",
      "App\\Middlewares\\": "app/middlewares/"
    }
  },
  "autoload-dev": {
    "files": [
      "./ide.php"
    ]
  }
}
